{
  "name": "jamstack-web-castanya",
  "version": "1.0.0",
  "description": "JAMStack website with Eleventy, TinaCMS, and e-commerce functionality for chestnuts business.",
  "main": ".eleventy.js",
  "scripts": {
    "dev": "eleventy --serve",
    "build": "eleventy",
    "tina:dev": "tinacms dev -c \"npm run dev\"",
    "tina:build": "tinacms build",
    "format": "prettier --write .",
    "deploy": "npm run build && netlify deploy --prod --dir=_site",
    "clean": "rm -rf _site"
  },
  "keywords": [
    "jamstack",
    "eleventy",
    "tinacms",
    "ecommerce",
    "blog",
    "netlify"
  ],
  "author": "Sergio Campos",
  "license": "Apache 2.0",
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.2.0",
    "@tinacms/cli": "^2.1.7",
    "@types/node": "^24.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.0.0",
    "html-minifier": "^4.0.0",
    "luxon": "^3.4.3",
    "markdown-it": "^14.1.1",
    "markdown-it-anchor": "^8.6.7",
    "netlify-cli": "^24.0.1",
    "prettier": "^3.0.3",
    "sass": "^1.67.0"
  },
  "dependencies": {
    "@sendgrid/mail": "^8.1.6",
    "@tinacms/auth": "^1.0.2",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.3.1",
    "node-fetch": "^3.3.2"
  },
  "engines": {
    "node": "^24.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/campossrg/jamstack_web_castanya.git"
  }
}
